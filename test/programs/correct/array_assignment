main() {
    const array int[] unidimensional;
    const array int[][] bidimensional;
    var array int[][][] tridimensional;

    unidimensional = {3, 7, 8};
    bidimensional = {{1,2},{3,4}};
    tridimensional =
    {
        {
            {10 ,20 ,99},
            {30 ,40 ,88}
        },
        {
            {50 ,60 ,77},
            {80 ,70 ,66}
        }
    };

    unidimensional[3] = 7;
    bidimensional[0][1] = 7;
    bidimensional[0] = {7, 8};
    tridimensional[7][3][4] = 7;
}
