function bubbleSort int[](int[] arr) {
    var int n = length(arr);
    var int i = 0;
    var boolean swapped = false;
    while (i < n && !swapped) {
        swapped = true;
        var int j = 0;
        while (j < n) {
            if (arr[j] > arr[j + 1]) {
                swapped = false;
                var int aux = arr[j];
                arr[j] = arr[j + 1];
                arr[j + 1] = aux;
            }
            j = j + 1;
        }
        i = i + 1;
    }
    return arr;
}

main() {
    var array int[] result;
    result = bubbleSort({1,9,3,3,5,12,10,1});
    print(result);
}
