function twoSum int[](int[] nums, int n, int target) {
    var array int[] result;
    var int i = 0;
    var boolean res = false;
    while (i < n && !res) {
        var int j = 0;
        while (j < n) {
            if (nums[i] == target - nums[i]) {
                result = numToArray(i, j);
                res = true;
            }
            j = j + 1;
        }
        i = i + 1;
    }
    if (!res) {
        result = numToArray(-1, -1);
        print("No solution");
    }
    return result; 
}

function numToArray int[](int a, int b) {
    var array int[] result = {0, 0};
    result[0] = a;
    result[1] = b;
    return result;
}

main() {
    const array int[] nums = {3, 4, 5};
    const int n = 3;
    print(twoSum(nums, n, 9));
}