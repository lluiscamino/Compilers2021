function twoSum int[](int[] nums, int target) {
    var array int[] result;
    var int i = 0;
    var boolean res = false;
    while (i < length(nums) && !res) {
        var int j = 0;
        while (j < length(nums)) {
            if (nums[i] == target - nums[i]) {
                result = numToArray(i, j);
                res = true;
            }
            j = j + 1;
        }
        i = i + 1;
    }
    if (!res) {
        result = {-1, -1};
        print("No solution");
    }
    return result; 
}

function numToArray int[](int a, int b) {
    var array int[] result = {a, b};
    return result;
}

main() {
    const array int[] nums = {3, 4, 5};
    print(twoSum(nums, 9));
}